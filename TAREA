        "mes_nac = int(input(\"Mes de nacimiento: \"))\n",
        "año_nac = int(input(\"Año de nacimiento: \"))\n",
        "\n",
        "dia_act = int(input(\"Día actual: \"))\n",
        "mes_act = int(input(\"Mes actual: \"))\n",
        "año_act = int(input(\"Año actual: \"))\n",
        "\n",
        "edad = año_act - año_nac\n",
        "\n",
        "cumple_este_año = año_act * 10000 + mes_nac * 100 + dia_nac\n",
        "fecha_actual = año_act * 10000 + mes_act * 100 + dia_act\n",
        "\n",
        "no_cumplio = (cumple_este_año - fecha_actual) // 100000000 + 1\n",
        "edad = edad - no_cumplio\n",
        "\n",
        "print(\"Edad\", edad)\n"
